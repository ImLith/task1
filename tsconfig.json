{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-native",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "assets/*": ["assets/*"],
      "<pages>/*": ["src/pages/*"],
      "<routes>/*": ["src/routes/*"],
      "<constants>/*": ["src/constants/*"],
      "<types>/*": ["src/types/*"],
      "<utils>/*": ["src/utils/*"],
      "<components>/*": ["src/components/*"]
    }
  },
  "exclude": ["node_modules"]
}
